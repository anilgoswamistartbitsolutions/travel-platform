{
    "buildCommand": "pnpm run build",
    "installCommand": "pnpm install",
    "functions": {
      "api/payload.js": {
        "runtime": "nodejs18.x"
      },
      "api/test.js": {
        "runtime": "nodejs18.x"
      }
    },
    "rewrites": [
      {
        "source": "/admin(.*)",
        "destination": "/api/payload"
      },
      {
        "source": "/api(.*)",
        "destination": "/api/payload"
      },
      {
        "source": "/(.*)",
        "destination": "/api/payload"
      }
    ]
  }